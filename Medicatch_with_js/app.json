{
  "expo": {
    "name": "Medicatch",
    "scheme": "Medicatch",
    "slug": "Medicatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8f5fa"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo1.png",
        "backgroundColor": "#e8f5fa"
      },
      "package": "com.fyp.medicatch",
      "googleServicesFile":"./google-services.json"
    },
    "web": {
      "favicon": "./assets/logo1.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3931274c-dc64-4624-a0ae-9708dcd25139"
      }
    },
    "owner": "muhammadsaqibali"
  }
}
