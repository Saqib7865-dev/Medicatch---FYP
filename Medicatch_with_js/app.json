{
  "expo": {
    "name": "Medicatch",
    "scheme": "Medicatch",
    "slug": "Medicatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8f5fa"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo1.png",
        "backgroundColor": "#e8f5fa"
      }
    },
    "web": {
      "favicon": "./assets/logo1.png"
    },
    "plugins": ["expo-router"]
  }
}
